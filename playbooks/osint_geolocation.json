            {"name": "OSINTgram", "url": "https://github.com/Datalux/Osintgram", "description": "Instagram OSINT"}
        ],
        "osint_frameworks": [
            {"name": "OSINT Framework", "url": "https://osintframework.com/", "description": "Collection of OSINT tools"},
            {"name": "IntelX", "url": "https://intelx.io/", "description": "Intelligence search engine"},
            {"name": "SpiderFoot", "url": "https://www.spiderfoot.net/", "description": "OSINT automation"},
            {"name": "Maltego", "url": "https://www.maltego.com/", "description": "Link analysis tool"}
        ]
    },
    "flag_patterns": [
        "digitalcyberhunt\\{[^}]+\\}",
        "DCH\\{[^}]+\\}",
        "flag\\{[^}]+\\}",
        "FLAG\\{[^}]+\\}",
        "CTF\\{[^}]+\\}"
    ],
