            {"name": "Censys", "url": "https://search.censys.io/", "description": "Internet-wide scanning"},
            {"name": "VirusTotal", "url": "https://www.virustotal.com/", "description": "URL/IP/file analysis"},
            {"name": "SecurityTrails", "url": "https://securitytrails.com/", "description": "DNS history"},
