{
    "name": "OSINT & Geolocation Analysis",
    "description": "Extract GPS coordinates, metadata, and OSINT data from images and files - Includes Online OSINT tools",
    "category": "osint",
    "file_types": [".jpg", ".jpeg", ".png", ".bmp", ".gif", ".tiff", ".webp", ".heic"],
    "execution_strategy": "sequential",
    "methods": [
        {
            "name": "GPS Extraction",
            "description": "Extract GPS coordinates from image EXIF",
            "type": "tool",
            "tool": "exiftool",
            "args": ["-gps*", "-c", "%.6f", "{target}"],
            "timeout": 30,
            "flag_search": true
        },
        {
            "name": "Full Metadata Dump",
            "description": "Extract all metadata for OSINT analysis",
            "type": "tool",
            "tool": "exiftool",
            "args": ["-a", "-u", "-g1", "{target}"],
            "timeout": 30,
            "flag_search": true
        },
        {
            "name": "Camera & Device Info",
            "description": "Extract camera make, model, serial numbers",
            "type": "tool",
            "tool": "exiftool",
            "args": ["-Make", "-Model", "-SerialNumber", "-LensModel", "-Software", "{target}"],
            "timeout": 30,
            "flag_search": true
        },
        {
            "name": "Author & Creator Info",
            "description": "Look for creator/author information",
            "type": "tool",
            "tool": "exiftool",
            "args": ["-Author", "-Creator", "-Artist", "-Copyright", "-OwnerName", "-Comment", "-UserComment", "{target}"],
            "timeout": 30,
            "flag_search": true
        },
        {
            "name": "Timestamp Analysis",
            "description": "Extract all timestamps for timeline",
            "type": "tool",
            "tool": "exiftool",
            "args": ["-time:all", "-d", "%Y-%m-%d %H:%M:%S", "{target}"],
            "timeout": 30,
            "flag_search": true
        },
        {
            "name": "String Search",
            "description": "Search for hidden text and coordinates",
            "type": "tool",
            "tool": "strings",
            "args": ["-n", "6", "{target}"],
            "timeout": 60,
            "flag_search": true
        },
        {
            "name": "Hex Analysis",
            "description": "Look for hidden data in hex",
            "type": "tool",
            "tool": "xxd",
            "args": ["{target}"],
            "timeout": 60,
            "flag_search": true
        },
        {
            "name": "Binwalk Scan",
            "description": "Check for embedded files",
            "type": "tool",
            "tool": "binwalk",
            "args": ["-e", "{target}"],
            "timeout": 120,
            "flag_search": true
        }
    ],
    "online_osint_tools": {
        "reverse_image_search": [
            {"name": "Google Images", "url": "https://images.google.com/", "description": "Click camera icon to upload"},
            {"name": "TinEye", "url": "https://tineye.com/", "description": "Reverse image search engine"},
            {"name": "Yandex Images", "url": "https://yandex.com/images/", "description": "Often finds results Google misses"},
            {"name": "Bing Visual Search", "url": "https://www.bing.com/visualsearch", "description": "Microsoft image search"},
            {"name": "PimEyes", "url": "https://pimeyes.com/", "description": "Face recognition search"},
            {"name": "FaceCheck.ID", "url": "https://facecheck.id/", "description": "Face search engine"}
        ],
        "geolocation": [
            {"name": "What3Words", "url": "https://what3words.com/", "description": "3-word address to coordinates"},
            {"name": "Plus Codes", "url": "https://plus.codes/", "description": "Open Location Code decoder"},
            {"name": "GeoSpy AI", "url": "https://geospy.ai/", "description": "AI-powered image geolocation"},
            {"name": "Picarta", "url": "https://picarta.ai/", "description": "AI photo location finder"},
            {"name": "GeoHack", "url": "https://geohack.toolforge.org/", "description": "Coordinate converter"}
